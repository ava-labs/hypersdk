{
  "name": "sample-metamask-snap-for-hypersdk-v2",
  "version": "0.1.1",
  "description": "The 'Hello, world!' of MetaMask Snaps, now written in TypeScript.",
  "repository": {
    "type": "git",
    "url": "https://github.com/MetaMask/template-snap-monorepo.git"
  },
  "license": "NOT_DECIDED_YET_TODO_FIX",
  "main": "./dist/bundle.js",
  "files": [
    "dist/",
    "snap.manifest.json"
  ],
  "scripts": {
    "build": "mm-snap build",
    "build:clean": "yarn clean && yarn build",
    "clean": "rimraf dist",
    "lint": "yarn lint:eslint && yarn lint:misc --check",
    "lint:eslint": "eslint . --cache --ext js,ts",
    "lint:fix": "yarn lint:eslint --fix && yarn lint:misc --write",
    "lint:misc": "prettier '**/*.json' '**/*.md' '!CHANGELOG.md' --ignore-path .gitignore",
    "prepublishOnly": "mm-snap manifest",
    "serve": "mm-snap serve",
    "start": "mm-snap watch",
    "test": "jest",
    "dev": "mm-snap watch",
    "deploy": "npm run build && npm publish"
  },
  "dependencies": {
    "@metamask/key-tree": "^9.1.2",
    "@metamask/snaps-sdk": "^4.0.0",
    "@scure/base": "^1.1.7",
    "buffer": "^6.0.3",
    "lossless-json": "^4.0.1",
    "tweetnacl": "^1.0.3"
  },
  "devDependencies": {
    "@jest/globals": "^29.2.3",
    "@metamask/auto-changelog": "^3.4.4",
    "@metamask/eslint-config": "^12.2.0",
    "@metamask/eslint-config-jest": "^12.1.0",
    "@metamask/eslint-config-nodejs": "^12.1.0",
    "@metamask/eslint-config-typescript": "^12.1.0",
    "@metamask/snaps-cli": "^6.1.0",
    "@metamask/snaps-jest": "^7.0.0",
    "@types/jest": "^29.5.12",
    "@typescript-eslint/eslint-plugin": "^5.42.1",
    "@typescript-eslint/parser": "^5.42.1",
    "eslint": "^8.45.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-import": "~2.26.0",
    "eslint-plugin-jest": "^27.1.5",
    "eslint-plugin-jsdoc": "^41.1.2",
    "eslint-plugin-n": "^15.7.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-promise": "^6.1.1",
    "jest": "^29.2.3",
    "prettier": "^2.7.1",
    "prettier-plugin-packagejson": "^2.2.11",
    "rimraf": "^3.0.2",
    "ts-jest": "^29.2.3",
    "typescript": "^5.1.0"
  },
  "engines": {
    "node": ">=18.6.0"
